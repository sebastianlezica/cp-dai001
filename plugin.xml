<?xml version="1.0" encoding="UTF-8"?>
<plugin xmlns="http://www.phonegap.com/ns/plugins/1.0"
    xmlns:android="http://schemas.android.com/apk/res/android"
    id="daicomplements"
    version="0.0.1">

    <name>DAI Complements</name>
    <description></description>
    <author></author>
    <license>MIT</license>
    <keywords>ecosystem:cordova</keywords>
    <engines>
        <engine name="cordova" version=">=3.0.0" />
    </engines>

    <platform name="android">
        <config-file target="AndroidManifest.xml" parent="/manifest">
            <uses-permission android:name="android.permission.REQUEST_IGNORE_BATTERY_OPTIMIZATIONS" />
        </config-file>
        <config-file target="config.xml" parent="/*">
            <feature name="DaiComplements" >
                <param name="android-package" value="cordova.plugins.DaiComplements"/>
            </feature>
        </config-file>

        <js-module src="daicomplements.js" name="Diagnostic">
            <clobbers target="cordova.plugins.DaiComplements" />
        </js-module>
        <source-file src="DaiComplements.java" target-dir="src/cordova/plugins" />
    </platform>
</plugin>
